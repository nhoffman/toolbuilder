      },
      "cancer_length": {
        "type": "number",
        "description": "Length of the cancer within the biopsy core"
      },
      "diagnosis": {
        "type": "string",
        "description": "Diagnosis of the biopsy",
        "enum": [
          "malignant",
          "benign"
        ]
      }
    },
    "required": [
      "gleason_score",
      "diagnosis"
    ]
  },
  "type": "function",
  "description": "Provide details about each biopsy in the report. "
}